CONFIGURE_FLAGS="
  CC=gcc \
  CXX=g++ \
  F77=gfortran \
  CXXFLAGS='-fno-strict-aliasing -DDUNE_STUFF_FUNCTIONS_TO_LIB -DDEBUG -g3 -ggdb -std=c++0x -Wextra -Wall -DDUNE_GRID_EXPERIMENTAL_GRID_EXTENSIONS=1' \
  --enable-parallel \
  --with-alugrid=/opt/dune/modules/alugrid/alugrid-1.52 \
  --disable-documentation \
  INKSCAPE=no \
"
CMAKE_FLAGS="-Wno-dev"
BUILDDIR=build-gcc-debug
